<template name="2f20500d">
  <view class="_view data-v-b246dcd8 page">
    <view class="_view data-v-b246dcd8 uni-list user-header">
      <navigator url="userinfo" class="_navigator data-v-b246dcd8">
        <view class="_view data-v-b246dcd8 uni-list-cell" hover-class="uni-list-cell-hover">
          <view class="_view data-v-b246dcd8 uni-media-list">
            <view class="_view data-v-b246dcd8 uni-media-list-logo">
              <image src="{{userinfo.img}}" class="_image data-v-b246dcd8"></image>
            </view>
            <view class="_view data-v-b246dcd8 uni-media-list-body">
              <view class="_view data-v-b246dcd8 uni-media-list-text-top">{{userinfo.nickname}} </view>
              <view class="_view data-v-b246dcd8 uni-media-list-text-bottom uni-ellipsis">{{userinfo.content}}</view>
            </view>
          </view>
        </view>
      </navigator>
    </view>
    <view class="_view data-v-b246dcd8 uni-list-cell-divider"></view>
    <view class="_view data-v-b246dcd8 uni-card">
      <view class="_view data-v-b246dcd8 uni-card-header">我的资产</view>
      <view class="_view data-v-b246dcd8 uni-card-content">
        <view class="_view data-v-b246dcd8 uni-grid-16">
          <navigator class="_navigator data-v-b246dcd8 uni-grid-16-item {{index % 4 === 3 ? 'no-border-right' : ''}}" hover-class="uni-grid-16-item-hover" wx:key="index" key="{{index}}" url="{{item.link}}" wx:for="{{myAssets}}" wx:for-index="index" wx:for-item="item">
            <image class="_image data-v-b246dcd8 uni-grid-16-image" src="{{item.icon}}"></image>
            <text class="_text data-v-b246dcd8 uni-grid-16-text">{{item.title}}</text>
          </navigator>
        </view>
      </view>
    </view>
    <view class="_view data-v-b246dcd8 uni-list-cell-divider"></view>
    <view class="_view data-v-b246dcd8 opennewworld">
      <button type="primary" size="{{primarySize}}" plain="{{plain}}" class="_button data-v-b246dcd8"> 开启新世界 </button>
    </view>
    <view class="_view data-v-b246dcd8 uni-list-cell-divider"></view>
    <view class="_view data-v-b246dcd8 uni-card">
      <view class="_view data-v-b246dcd8 uni-card-header">新世界</view>
      <view class="_view data-v-b246dcd8 uni-card-content">
        <view class="_view data-v-b246dcd8 uni-grid-16">
          <view class="_view data-v-b246dcd8 uni-grid-16-item {{index % 4 === 3 ? 'no-border-right' : ''}}" hover-class="uni-grid-16-item-hover" wx:key="index" key="{{index}}" wx:for="{{newWorld}}" wx:for-index="index" wx:for-item="item">
            <image class="_image data-v-b246dcd8 uni-grid-16-image" src="{{item.icon}}"></image>
            <text class="_text data-v-b246dcd8 uni-grid-16-text">{{item.title}}</text>
          </view>
        </view>
      </view>
    </view>
    <view class="_view data-v-b246dcd8 uni-list-cell-divider"></view>
    <view class="_view data-v-b246dcd8 uni-card">
      <view class="_view data-v-b246dcd8 uni-card-header">我的新世界</view>
      <view class="_view data-v-b246dcd8 uni-card-content">
        <view class="_view data-v-b246dcd8 uni-grid-16">
          <view class="_view data-v-b246dcd8 uni-grid-16-item {{index % 4 === 3 ? 'no-border-right' : ''}}" hover-class="uni-grid-16-item-hover" wx:key="index" key="{{index}}" wx:for="{{myContribution}}" wx:for-index="index" wx:for-item="item">
            <image class="_image data-v-b246dcd8 uni-grid-16-image" src="{{item.icon}}"></image>
            <text class="_text data-v-b246dcd8 uni-grid-16-text">{{item.title}}</text>
          </view>
        </view>
      </view>
    </view>
    <view class="_view data-v-b246dcd8 uni-list-cell-divider"></view>
  </view>
</template>